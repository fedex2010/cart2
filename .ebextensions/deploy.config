container_commands:
  00_copy_index_garba_to_s3:
    command: aws s3 sync --acl public-read-write client/build-garba s3://cart-ui-ci.garbarino.com/carrito --delete
    leader_only: true
  01_copy_index_compu_to_s3:
    command: aws s3 sync --acl public-read-write client/build-compu s3://cart-ui-ci.compumundo.com.ar/carrito --delete
    leader_only: true
  02_copy_build_garba_to_s3:
    command: aws s3 sync --acl public-read-write client/build-garba s3://garbarino-fe/cart-ui/garbarino --delete
    leader_only: true
  03_copy_build_compu_to_s3:
    command: aws s3 sync --acl public-read-write client/build-compu s3://garbarino-fe/cart-ui/compumundo --delete
    leader_only: true