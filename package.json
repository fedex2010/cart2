{
  "name": "cart-ui",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "install-client": "cd client && yarn install",
    "install-server": "cd server && yarn install",
    "postinstall": "npm-run-all install-server",
    "norma-garba": "cd client && node normaJob.js garbarino",
    "norma-compu": "cd client && node normaJob.js compumundo",
    "server-dev": "cd server && yarn run server-dev",
    "server-prod": "cd server && yarn run server-prod",
    "server-staging": "cd server && yarn run server-staging",
    "server-ci": "cd server && yarn run server-ci",
    "client": "cd client && yarn run start",
    "garba-cart": "yarn run norma-garba && npm-run-all --parallel client server-dev",
    "compu-cart": "yarn run norma-garba && npm-run-all --parallel client server-dev",
    "build-client": "cd client && yarn build",
    "mv-client-garba": "cd client && mv build build-garba",
    "mv-client-compu": "cd client && mv build build-compu",
    "build-garba": "yarn norma-garba && yarn build-client && yarn mv-client-garba",
    "build-compu": "yarn norma-compu && yarn build-client && yarn mv-client-compu"
  },
  "keywords": [],
