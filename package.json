{
  "name": "cart-ui",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "install-client": "cd client && yarn install",
    "install-server": "cd server && yarn install",
    "postinstall": "npm-run-all install-server install-client",
    "client": "cd client && yarn run start",
    "client-ci": "cd client && yarn run build && yarn run deploy-ci",
    "server-dev": "cd server && yarn run dev",
    "server": "cd server && yarn run start",
    "server-ci": "cd server && yarn run server-ci",
    "normaGBJob": "cd client && node normaJob.js garbarino",
    "normaCMJob": "cd client && node normaJob.js compumundo",
    "garba-cart": "yarn run normaGBJob && npm-run-all --parallel client server-dev",
    "compu-cart": "yarn run normaCMJob && npm-run-all --parallel client server-dev",
    "start-ci": "npm-run-all --parallel server-ci deploy-client-ci",
    "deploy-client-ci": "yarn normaGBJob && yarn client-ci"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "node-fetch": "^2.3.0",
    "npm-run-all": "^4.1.5"
  }
}
