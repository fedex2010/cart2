{
  "name": "cart-ui",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "prune-client": "cd client && npm prune",
    "prune-server": "cd server && npm prune",
    "clean": "npm-run-all prune-server prune-client",
    "install-client": "cd client && npm install",
    "install-server": "cd server && npm install",
    "postinstall": "npm run clean && npm-run-all install-server install-client",
    "client": "cd client && npm run start",
    "server-dev": "cd server && npm run dev",
    "server": "cd server && npm run start",
    "normaGBJob": "cd client && node normaJob.js garbarino",
    "normaCMJob": "cd client && node normaJob.js compumundo",
    "garba-cart": "npm run normaGBJob && npm-run-all --parallel client server-dev",
    "compu-cart": "npm run normaCMJob && npm-run-all --parallel client server-dev",
    "start-ci": "npm run normaGBJob && npm-run-all --parallel client server"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "node-fetch": "^2.3.0",
    "npm-run-all": "^4.1.5"
  },
  "devDependencies": {
    "swiper": "^4.4.6"
  }
}
